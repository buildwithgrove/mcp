{
  "services": [
    {
      "id": "ethereum-mainnet",
      "name": "Ethereum Mainnet",
      "blockchain": "ethereum",
      "network": "mainnet",
      "rpcUrl": "https://eth.rpc.grove.city/v1/01fdb492",
      "protocol": "json-rpc",
      "category": "evm",
      "supportedMethods": [
        {
          "name": "eth_blockNumber",
          "description": "Returns the number of the most recent block (latest height)",
          "params": [],
          "example": {"jsonrpc": "2.0", "method": "eth_blockNumber", "params": [], "id": 1},
          "category": "block"
        },
        {
          "name": "eth_getBlockByNumber",
          "description": "Returns information about a block by block number",
          "params": [
            {"name": "blockNumber", "type": "string", "description": "Block number in hex or 'latest'", "required": true},
            {"name": "fullTransactions", "type": "boolean", "description": "If true, returns full transaction objects", "required": true, "default": false}
          ],
          "example": {"jsonrpc": "2.0", "method": "eth_getBlockByNumber", "params": ["latest", false], "id": 1},
          "category": "block"
        },
        {
          "name": "eth_getBalance",
          "description": "Returns the balance of an account",
          "params": [
            {"name": "address", "type": "string", "description": "Address to check balance", "required": true},
            {"name": "blockNumber", "type": "string", "description": "Block number or 'latest'", "required": false, "default": "latest"}
          ],
          "example": {"jsonrpc": "2.0", "method": "eth_getBalance", "params": ["0x...", "latest"], "id": 1},
          "category": "account"
        },
        {
          "name": "eth_call",
          "description": "Executes a new message call immediately without creating a transaction",
          "params": [
            {"name": "transaction", "type": "object", "description": "Transaction call object", "required": true},
            {"name": "blockNumber", "type": "string", "description": "Block number or 'latest'", "required": false, "default": "latest"}
          ],
          "category": "transaction"
        },
        {
          "name": "eth_sendRawTransaction",
          "description": "Creates new message call transaction or a contract creation for signed transactions",
          "params": [
            {"name": "data", "type": "string", "description": "Signed transaction data", "required": true}
          ],
          "category": "transaction"
        },
        {
          "name": "eth_getLogs",
          "description": "Returns an array of all logs matching a given filter object",
          "params": [
            {"name": "filter", "type": "object", "description": "Filter options", "required": true}
          ],
          "category": "logs"
        }
      ]
    },
